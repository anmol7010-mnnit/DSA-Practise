#include <bits/stdc++.h>
using namespace std;

int main()
{
    int a,b,c,d;
    cin >> a >> b >> c >> d;
    
    int first = 3*a/10;
    int second = a - (a/250)*c;
    
    int third = 3*b/10;
    int fourth = b - (b/250)*d;
    
    int one = max(first,second);
    int two = max(third,fourth);
    
    if (one > two) {
        cout << "Misha" << endl;
    }
    else if (two > one) {
        cout << "Vasya" << endl;
    }
    else {
        cout << "Tie" << endl;  
    }
}
