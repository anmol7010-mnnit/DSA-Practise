#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin >> t;
    
    while (t--) {
        
        string ans = "NO";
        
        int n,k;
        cin >> n >> k;
        
        int arr[n];
        for (int i = 0; i < n; i++) {
            cin >> arr[i];
        }
        
        for (int i = 0; i < n; i++) {
           if (arr[i] == k) {
               ans = "YES";
           }
        }
        
        cout << ans << endl;
    }
}
