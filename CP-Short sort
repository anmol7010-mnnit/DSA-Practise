#include <iostream>
using namespace std;
 
int main()
{
    int t;
    cin >> t;
    while (t--) {
        
        string ans = "NO";
        
        string s;
        cin >> s;
        
        if (s == "abc") {
            ans = "YES";
        }
        
        if (s[0] == 'a' || s[1] == 'b' || s[2] == 'c') {
            ans = "YES";
        }
        
        cout << ans << endl;
    }
}
